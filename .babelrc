        "bugfixes": true,
        "spec":true,
        "modules":false
      }
    ]
  ],
  "plugins": ["@babel/plugin-transform-typescript",
    ["module-resolver", {
      "root": ["./lib"],
      "alias": {
        "gl-matrix": "./libs/glMatrix/index.js"
      }
    }]
  ]
}